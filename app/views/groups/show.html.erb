<div class="page-header">
  <h2><%= @group.name %></h2>
</div>

<div class="row col-md-12">
    <dl class="dl-horizontal">
      <dt>Average</dt>
      <dd><%= @group.mean %></dd>

      <dt>Median</dt>
      <dd><%= @group.median %></dd>

      <dt>Standard Deviation</dt>
      <dd><%= @group.std_dev %></dd>

      <dt>5th Percentile</dt>
      <dd><%= @group.five %></dd>

      <dt>25th Percentile</dt>
      <dd><%= @group.twentyfive %></dd>

      <dt>50th Percentile</dt>
      <dd><%= @group.fifty %></dd>

      <dt>75th Percentile</dt>
      <dd><%= @group.seventyfive %></dd>

      <dt>95th Percentile</dt>
      <dd><%= @group.ninetyfive %></dd>

      <dt><br /></dt>
      <dd><br /></dd>

      <ol class="breadcrumb">
        <li><a href="/groups">Back</a></li>
        <li><a href="/funds/">List of Funds</a></li>
      </ol>
    </dl>
</div>


